<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sbs.example.lolHi.dao.MemberDao">
	<select id="getMembers" resultType="Member">
		SELECT *
		FROM member
		ORDER BY
		id DESC
	</select>


	<insert id="joinMember" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO member
		<set>
			regDate = NOW(),
			updateDate = NOW(),
			loginId = #{loginId},
			`loginPw` = #{loginPw},
		</set>
	</insert>
</mapper>
